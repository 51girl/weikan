        <type id="size" name="技术规格">
            <size id="hardware" name="硬件">
                <item name="尺寸标注(W×D×H)">95×220×65</item>
                <item name="机器类型">手持式热转移标签打印机</item>
                <item name="重量">454g</item>
                <item name="键盘">按字母顺序排列</item>
                <item name="液晶屏">2.6英寸 2行*6字符</item>
                <item name="剪切">手动剪切</item>
                <item name="电源">DC IN 9.5V，6节5号干电池</item>
                <item name="接口">USB</item>
                <item name="打印高度">24mm</item>
                <item name="色带宽度(mm)">6、9、12、14、18、24</item>
                <item name="打印方式">热转印</item>
                <item name="打印精度">203dpi</item>
                <item name="打印速度">10mm/s</item>
                <item name="随机附件">用户手册、合格证、保修卡、V-TYPE标签色带（12mm 1盒）、USB数据线、CD-ROM 光盘(VariMark Label Editer 标签编辑软件)、电源适配器</item>
            </size>
            <size id="features" name="特性">
                <item name="字体尺寸">6种</item>
                <item name="序号打印">0-99</item>
                <item name="多份打印">1-99</item>
                <item name="竖版打印">有</item>
                <item name="定长打印">30-400mm</item>
                <item name="打印行数">最多6行（24mmV-TYPE标签色带）</item>
                <item name="边框">有</item>
                <item name="表格">有</item>
                <item name="符号">9大类</item>
                <item name="文件存储">20个文件</item>
                <item name="打印预览">有</item>
                <item name="条形码">内置8种条形码</item>
                <item name="热缩套管耗材">支持</item>
                <item name="模板">有</item>
                <item name="余白调整">有</item>
            </size>
            <size id="software" name="随机软件">
                <item name="随机软件">VariMark Label Editer 标签编辑软件</item>
                <item name="操作系统">Microsoft ® Windows® XP / Windows Vista® / Windows® 7 / Windows® 8</item>
            </size>
        </type>
